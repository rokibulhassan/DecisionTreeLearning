!!!
%html
  %head
    %meta{:charset => "utf-8"}
      %meta{:content => "width=device-width, initial-scale=1.0", :name => "viewport"}
        %meta{:content => "", :name => "description"}
          %meta{:content => "", :name => "author"}
            %title DecisionTree
            = javascript_include_tag "https://www.google.com/jsapi"
            = stylesheet_link_tag "application", :media => "all", "data-turbolinks-track" => true
            = javascript_include_tag "application", "data-turbolinks-track" => true
            = csrf_meta_tags
  %body
    .container
      %nav.navbar.navbar-inverse{:role => "navigation"}
        .navbar-header
          %button.navbar-toggle{"data-target" => ".navbar-ex1-collapse", "data-toggle" => "collapse", :type => "button"}
            %span.sr-only Toggle navigation
            %span.icon-bar
            %span.icon-bar
            %span.icon-bar
          %a.navbar-brand{:href => "/", :'data-no-turbolink' => true} Decision Tree
        .collapse.navbar-collapse.navbar-ex1-collapse
          %ul.nav.navbar-nav.navbar-right.navbar-user
            %li= link_to 'Generate Tree', id3_tree_classifications_path, :'data-no-turbolink' => true, :target => '_blank'
            %li= link_to 'Decision Tree', predict_classifications_path
            %li= link_to 'KNN Numerical', knn_numerical_classifications_path
            %li= link_to 'SVM', svm_classifications_path

    .container
      = yield
      .open-modal
    %footer
      .container.text-center
